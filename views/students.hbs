{{#if students.length}}
<table class="table table-dark">
    <thead>
        <tr>
            <th>Name</th>
            <th>Age</th>
            <th>Group Id</th>
        </tr>
    </thead>
    <tbody>
        {{#each students}}
        <tr>
            <th>{{name}}</th>
            <th>{{age}}</th>
            <th>{{groupId}}</th>
        </tr>
        {{/each}}
    </tbody>
</table>

{{else}}
<p>No students yet</p>
{{/if}}
<a href="students/add" class="btn btn-outline-info">Add</a>